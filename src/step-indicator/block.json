{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "jurible/step-indicator",
	"version": "1.0.0",
	"title": "Step Indicator",
	"category": "widgets",
	"icon": "editor-ol",
